<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">

        <!-- ✅ PC IP mo (XAMPP server) -->
        <domain includeSubdomains="true">192.168.1.11</domain>

        <!-- ✅ Optional: allow local addresses you already had -->
        <domain includeSubdomains="true">10.0.2.2</domain>
        <domain includeSubdomains="true">localhost</domain>

        <!-- ✅ Optional: your ngrok domain (kung ginagamit mo pa) -->
        <domain includeSubdomains="true">hytographically-nondistorted-aurelia.ngrok-free.dev</domain>

    </domain-config>
</network-security-config>